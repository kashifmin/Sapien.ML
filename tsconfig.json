      "module": "commonjs",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "experimentalDecorators":true,
      "outDir": "temp",
      "rootDir":"",
      "sourceMap": true,
      "sourceRoot":"src/",
      "target":"es5",
      "listFiles":true,
      "baseUrl": "./src"
    },
    "exclude": [
      "build",
